    "FileSettings": {
        "DriverName": "${FILE_SETTINGS__DRIVER_NAME}",
        "Directory": "./data/",
        "EnablePublicLink": true,
        "PublicLinkSalt": "${FILE_SETTINGS__PUBLIC_LINK_SALT}",
        "ThumbnailWidth": 120,
        "ThumbnailHeight": 100,
        "PreviewWidth": 1024,
        "PreviewHeight": 0,
        "ProfileWidth": 128,
        "ProfileHeight": 128,
        "InitialFont": "luximbi.ttf",
